{
    "apiVersion": 2,
    "name": "my-excalidraw-block/editor",
    "title": "Excalidraw Block",
    "category": "widgets",
    "icon": "block-default",
    "description": "A Gutenberg block that embeds an Excalidraw editor in the backend and renders an SVG on the frontend.",
    "keywords": ["excalidraw", "diagram", "sketch"],
  
    "textdomain": "my-excalidraw-block",
    "editorScript": "file:./build/index.js",
    "editorStyle": "file:./build/index.css",
    "style": "file:./build/style-index.css",
  
    "attributes": {
      "excalidrawData": {
        "type": "string",
        "default": ""
      },
      "exportedSvg": {
        "type": "string",
        "default": ""
      },
      "aspectRatio": {
        "type": "string",
        "default": "auto"
      },
      "showBorder": {
        "type": "boolean",
        "default": false
      },
      "altText": {
        "type": "string",
        "default": ""
      },
      "caption": {
        "type": "string",
        "default": ""
      }
    },    
  
    "supports": {
      "html": false
    }
  }
  